{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/fastworld-marketplace-backend/src/",
  "monorepo": true,
  "root": "apps/fastworld-marketplace-backend",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/fastworld-marketplace-backend/tsconfig.app.json"
  },
  "projects": {
    "fastworld-marketplace-backend": {
      "type": "application",
      "root": "apps/fastworld-marketplace-backend",
      "entryFile": "main",
      "sourceRoot": "apps/fastworld-marketplace-backend/src",
      "compilerOptions": {
        "webpackConfigPath": "webpack.config.js",
        "deleteOutDir": true,
        "tsConfigPath": "apps/fastworld-marketplace-backend/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "webpackConfigPath": "webpack.config.js",
        "deleteOutDir": true,
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    }
  }
}